echo -e "$HEAD
This is a MIME-encapsulated message

--simple
Content-Type: text/html

<html>
<head>
<title>No smokes in $days days, $"$dollars" saved, $smoke cigarettes not smoked</title>
</head>
<body> 
It's been $days days, you avoided smoking $smoke cigarettes and saved $"$dollars".
<img src=\"data:image/jpeg;base64,$jpg\" />
</body>
</html>
--simple--" | sendmail -t





